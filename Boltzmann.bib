doi = {10.1016/j.jtbi.2009.07.040},
file = {:Users/zuck016/Library/Application Support/Mendeley Desktop/Downloaded/Polynikis, Hogan, di Bernardo - 2009 - Comparing different ODE modelling approaches for gene regulatory networks.pdf:pdf},
isbn = {1095-8541 (Electronic) 0022-5193 (Linking)},
