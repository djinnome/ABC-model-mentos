title = {{Thermodynamic analysis of regulation in metabolic networks using constraint-based modeling.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2873351{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {3},
