issn = {1748-7188},
journal = {Algorithms for Molecular Biology},
keywords = {Algorithms,Bioinformatics,Cellular and Medical Topics,Computational Biology/Bioinformatics,Physiological},
month = {dec},
number = {1},
pages = {23},
publisher = {BioMed Central},
title = {{Atom mapping with constraint programming}},
url = {http://almob.biomedcentral.com/articles/10.1186/s13015-014-0023-3},
