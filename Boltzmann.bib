@article{RyanS.McClure1&ChristopherC.Overall1&JasonE.McDermott1EricHill1LyeMeng4Markillie1LeeAnnMcCue1RonaldC.Taylor1MarcusLudwig2DonaldA.Bryant232015,
abstract = {Cyanobacterial regulation of gene expression must contend with a genome organization that lacks apparent functional context, as the majority of cellular processes and metabolic pathways are encoded by genes found at disparate locations across the genome and there exists relatively few transcription factors. In this study, global transcript abundance data from the model cyanobacterium Synechococcus sp. PCC 7002 grown under 42 different conditions was analyzed 25 using Context-Likelihood of Relatedness (CLR). The resulting network, organized into 11 2 modules, provided insight into transcriptional network topology as well as grouping genes by 6 27 28 function and linking their response to specific environmental variables. When used in conjunction with genome sequences the network allowed identification and expansion of novel 29 30 31 potential targets of both DNA binding proteins and sRNA regulators. These results offer a new perspective into the multi-level regulation that governs cellular adaptations of fast-growing physiologically robust cyanobacterium Synechococcus sp. PCC 7002 to changing environmental 32 33 variables. It also provides a methodological high-throughput approach to studying multi-scale regulatory mechanisms that operate in cyanobacteria. Finally, it provides valuable context for 34 35 integrating systems-level data to enhance gene grouping based on annotated function, especially in organisms where traditional context analyses cannot be implemented due to lack of operon- 36 based functional organization. 2},
author = {{Ryan S. McClure1{\&}, Christopher C. Overall1{\&}, Jason E. McDermott1, Eric Hill1, Lye Meng 4 Markillie1, Lee Ann McCue1, Ronald C. Taylor1, Marcus Ludwig2, Donald A. Bryant2, 3}, and 5 Alex S. Beliaev*1 6},
journal = {Nucl. Acids Res.},
keywords = {nucleic acids research},
title = {{Linking Transcriptomic Organization to the Regulatory Landscape of Cyanobacteria through Co-Expression Network Analysis}},
year = {2015}
}
