title = {{A Scalable Algorithm to Explore the Gibbs Energy Landscape of Genome-Scale Metabolic Networks}},
url = {http://dx.doi.org/10.1371/journal.pcbi.1002562{\%}5Cnhttp://www.ploscompbiol.org/article/fetchObjectAttachment.action?uri=info{\%}3Adoi{\%}2F10.1371{\%}2Fjournal.pcbi.1002562{\&}representation=PDF},
