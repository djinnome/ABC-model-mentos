author = {H{\"{a}}dicke, Oliver and Klamt, Steffen},
doi = {10.1038/srep39647},
file = {:Users/zuck016/Library/Application Support/Mendeley Desktop/Downloaded/H{\"{a}}dicke, Klamt - 2016 - EColiCore2 a reference network model of the central metabolism of Escherichia coli and relationships to its gen.pdf:pdf},
issn = {2045-2322},
journal = {Scientific Reports},
month = {jan},
pages = {39647},
title = {{EColiCore2: a reference network model of the central metabolism of Escherichia coli and relationships to its genome-scale parent model}},
url = {http://www.nature.com/articles/srep39647},
volume = {7},
year = {2017}
}
