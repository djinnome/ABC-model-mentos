@article{,
abstract = {Section 8.2 presented the technique of estimating input probabilities of a process that are as unbiased as possible but consistent with known averages values of one or more quantities. This technique, the Principle of Maximum Entropy, was developed there for the simple case of one constraint and three input events, in which case the technique can be carried out analytically. It is described here for a more general case. 9.1 Problem Setup Before the Principle of Maximum Entropy can be used the problem domain must be set up. In cases involving physical systems, this means that the various states in which the system can exist need to be identified, and all the parameters involved in the constraints known. For example, this might include the energy, electric charge, and other quantities associated with each of the quantum states. In applications to nonphysical systems, the various possible events have to be enumerated and the properties of each determined, particularly the values associated with each of the constraints. It is not assumed in this step which particular state the system is actually in (which state is " occupied "). Indeed it is assumed that we cannot ever know this with certainty, so we deal instead with probabilities. In this Chapter we will apply the general mathematical derivation to two extremely simplified and crude examples, one a business model, and the other a model of a physical system.},
file = {:Users/zuck016/Library/Application Support/Mendeley Desktop/Downloaded/Unknown - Unknown - Principle of Maximum Entropy.pdf:pdf},
title = {{Principle of Maximum Entropy}},
url = {http://www-mtl.mit.edu/Courses/6.050/2014/notes/chapter9.pdf}
}
